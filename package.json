    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "raw-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
